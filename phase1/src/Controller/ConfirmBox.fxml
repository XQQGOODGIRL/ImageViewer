<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="600.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="Controller.ConfirmBox">
    <children>
        <ListView fx:id="Tags" prefHeight="500.0" prefWidth="200.0"/>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="Confirm" minWidth="45.0" mnemonicParsing="false" onAction="#Confirm" prefHeight="14.0"
                        prefWidth="119.0" text="Confirm" GridPane.rowIndex="2"/>
                <Button fx:id="add" mnemonicParsing="false" onAction="#AddNewTag" prefHeight="27.0" prefWidth="102.0"
                        text="+" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="newTag"/>
            </children>
        </GridPane>
    </children>
</VBox>
